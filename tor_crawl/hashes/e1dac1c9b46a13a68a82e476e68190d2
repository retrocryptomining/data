		}); */  
	    $.getJSON("/shopCart/cart",function(data){
			$("#cartQuantity").text(data.allQuantity);
		});
	    $("#menu_mid").load("/banner/topMenuList");
    });
    function top_banner(){
        $("#top_banner").slideUp("slow");
    }
