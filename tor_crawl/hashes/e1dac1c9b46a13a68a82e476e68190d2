                        window.location.href="/order/queryBuyer";
					}

                }
            });
		}*/

        if(!menuId){
            menuId=40;
        }
        $.cookie('clicked_menu', menuId, {path:'/'});
    }

    function openDistributor (){
        //同意协议并选择分销商类型
        $("#distributionTopDiv").attr('style', 'display:block');
        $("#distributionTopNextStep").click(function () {
            var disType = $('input:radio[name="distributionTop"]:checked').val();
            if (disType == null) {
                alert("请选择分销商类型！");
                return false;
            }
            var url = "";
            if(disType == 1){
                url = "/distribute/register/initPerson";
            }else{
                url = "/distribute/register/initCompany";
            }
            window.location.href =url;
        })
        //关闭按钮
        $("#distributionTopDivClose").click(function () {
            $('#distributionTopDiv').attr('style', 'display:none');
        });
    }




