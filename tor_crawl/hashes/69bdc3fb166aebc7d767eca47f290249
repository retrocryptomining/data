	Shadowbox.init({
	    language: 'en',
	    players:  ['html', 'iframe']
	}); $(".prize_draw h2").hover(
		function() {
			area = $(this).attr('id'),
			$('#cell_'+area+' .white_img').stop().animate({opacity: "0"}, "slow");
		},
		function() {
			$('#cell_'+area+' .white_img').stop().animate({opacity: "1"}, "slow");
		}
	);
		
	
