                        <script>
function number_format (number, decimals, dec_point, thousands_sep) {
  number = (number + '').replace(/[^0-9+\-Ee.]/g, '');
  var n = !isFinite(+number) ? 0 : +number,
    prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),
    sep = (typeof thousands_sep === 'undefined') ? ',' : thousands_sep,
    dec = (typeof dec_point === 'undefined') ? '.' : dec_point,
    s = '',
    toFixedFix = function (n, prec) {
      var k = Math.pow(10, prec);
      return '' + Math.round(n * k) / k;
    };
  // Fix for IE parseFloat(0.55).toFixed(0) = 0;
  s = (prec ? toFixedFix(n, prec) : '' + Math.round(n)).split('.');
  if (s[0].length > 3) {
    s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
  }
  if ((s[1] || '').length < prec) {
    s[1] = s[1] || '';
    s[1] += new Array(prec - s[1].length + 1).join('0');
  }
  return s.join(dec);
}
var Slider_Values = $.parseJSON('{"1":{"count":20,"price":125,"interval":"Y"},"2":{"count":50,"price":100,"interval":"Y"},"3":{"count":100,"price":90,"interval":"Y"},"4":{"count":150,"price":80,"interval":"Y"},"5":{"count":200,"price":75,"interval":"Y"},"6":{"count":400,"price":70,"interval":"Y"},"7":{"count":600,"price":62.5,"interval":"Y"},"8":{"count":800,"price":60,"interval":"Y"},"9":{"count":1000,"price":57.5,"interval":"Y"},"10":{"count":1500,"price":50,"interval":"Y"},"11":{"count":2000,"price":45,"interval":"Y"},"12":{"count":4000,"price":40,"interval":"Y"},"13":{"count":10000,"price":35,"interval":"Y"}}');
var Packages = $.parseJSON('[{"id":"1","nev":"Alap SMTP csomag","ar":"9900"},{"id":"2","nev":"Profi SMTP csomag","ar":"14900"},{"id":"3","nev":"H\u00edrlev\u00e9l SMTP csomag (r\u00e9gi)","ar":"120000"},{"id":"4","nev":"H\u00edrlev\u00e9l k\u00fcld\u0151 csomag (egyenlegfelt\u00f6lt\u0151s)","ar":"0"}]');
</script>
