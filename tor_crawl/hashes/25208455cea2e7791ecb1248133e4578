                        $(document).ready(function()
                        { 
                          $("input[name='domain[1]']").change(function()
                          {
                            $("input[name=username]").val("smtp@"+$(this).val());
                          }).change();
                          function size(obj) {
                            var size = 0, key;
                            for (key in obj) {
                                if (obj.hasOwnProperty(key)) size++;
                            }
                            return size;
                          };
                          $("#slider").slider({
                              min: 1,
                              max: size(Slider_Values),
                              step: 1,
                              slide: function (event, ui) {
                                  $('.mail_count').html(number_format(parseInt(Slider_Values[ui.value].count)*1000*12, 0, ',', '.') + " darab");
                                  $(".tcalc_price").html(number_format(Slider_Values[ui.value].price/1000*Slider_Values[ui.value].count*1000*12, 0, ',', '.'));
                                  $(".perLevel").html(number_format(Slider_Values[ui.value].price/1000, 3, ',', '.'));
                                  $("input[name=mail_count]").val(parseInt(Slider_Values[ui.value].count)*1000);
                                  $("input[name=help_id]").val(ui.value);
                              }
                          });
                          
                                                    
                                                  });
                        $(".mail_count").html("240.000 darab");
                        </script>
