              <script type="text/javascript">
              var oTabGroup = new TabGroup();
              var oTabber = new Tabber('tab_btn_', 'tab_', 'tab_menu_bg_selected', 'tab_menu_bg', 'tab_div_visible', 'tab_div', 4, oTabGroup);
              var oTabberRight = new Tabber('tab_right_btn_', 'tab_right_', 'tab_menu_sml_bg_selected', 'tab_menu_sml_bg', 'tab_div_visible', 'tab_div', 3, oTabGroup);
              
              function showLogin()
              {
                $("#modalBg").show(0, function()
                    {
                      $(document).keyup(function(e)
                      {
                        // ESC
                        if(e.keyCode == 27)
                          hideModal();
                      });
                      $(this).click(function()
                      {
                        hideModal();
                      });
                    });
                    $("#loginForm").show();
              }
              
              function showModal(pid)
              {
                $("input[name=csomag_id]").val(pid);
                var pac;
                for(var i in Packages)
                {
                  if(Packages[i].id == pid)
                  {
                    pac = Packages[i];
                    break;
                  }
                }
                $("#modalBg").show(0, function()
                {
                  $(document).keyup(function(e)
                  {
                    // ESC
                    if(e.keyCode == 27)
                      hideModal();
                  });
                  if(pid == 4)
                  {
                    $(".levelszam_cont").show();
	                  $(".megr_levelszam").html(number_format(Slider_Values[$("input[name=help_id]").val()].count*1000*12, 0, ',', '.') + " darab");
	                  //$(".megr_inter").html(Slider_Values[$("input[name=help_id]").val()].interval=="Y"?"Éves":"Havi");
	                  $(".megr_osszeg").html(number_format(Slider_Values[$("input[name=help_id]").val()].price/1000*Slider_Values[$("input[name=help_id]").val()].count*1000*12, 0, ',', '.') + " Ft + ÁFA");                    
                  }
                  else
                  {
                    $(".levelszam_cont").hide();
                    $(".megr_inter").html("Éves");
                    $(".megr_osszeg").html(number_format(pac.ar, 0, ',', '.') + " Ft");
                  }
                  
                  if(pid == 2)
                  {
                    var i = 5;
                    for(i; i > 1; i--)
                    {
                      $(".domain:first").after("<tr class='plusDomains'><td><label>Domain #"+i+":</label></td><td><input type='text' name='domain["+i+"]' placeholder='azondomainneve.hu' /></td></tr>");
                    }
                    
                  }
                  
                  $(".pName").html(pac.nev);
                  $(this).click(function()
                  {
                    hideModal();
                  });
                });
                $("#hirlForm").show();
              }
              
              function hideModal()
              {
                $("#modalBg").hide();
                $("#hirlForm").hide();
                $("#loginForm").hide();
                $(".plusDomains").remove();
              }
              
              
              function ShowRegisterForPackage(iPackage)
              {
                showModal(iPackage);
                $("html, body").animate({
                  scrollTop: $("html").offset().top
                });
                return;
                
                var oCsomagID = document.getElementById('csomag_id');
                
                oCsomagID.value = iPackage;
                sendReq('domainlista', 'ajax.php', 'func=mutatDomainLista&args[csomag_id]='+iPackage);
                  
                oTabberRight.ShowTab(1);
              }
              oTabber.ShowTab(3);
              </script>
