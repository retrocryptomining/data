        <script type="text/javascript">
        $(document).ready(function()
        {
          oTabber.ShowTab(3);
          
       	  $("#checkCoupon").click(function(){
       	  	var btn = $(this);
       	  	var code = $("input[name=coupon]").val();
       	  	var pid = $("input[name=csomag_id]").val();
       	  	var pid2 = $("input[name=help_id]").val();
       	  	if(code == '') return false;
       	  	if($("input[name=felhasznalom]").val() != 0) return false;
       	  	$.post('ajax.php', {
       	  		'func' : 'checkCoupon',
       	  		'args' : {
       	  			'cid' : code,
       	  			'pid' : pid,
       	  			'pid2' : pid2
       	 		}
       	  	}, function(data){
       	  		try
       	  		{
       	  			var obj = $.parseJSON(data);
       	  			if(obj.error == 1) {
       	  				alert("Sikertelen kuponbeváltás. Érvénytelen kuponkód!");
       	  			} else {
       	  				btn.remove();
       	  				$("td.megr_osszeg").html(obj.price);
       	  				$("input[name=felhasznalom]").val(1);
       	  				$("input[name=coupon]").attr('name','coupon_bevaltva').attr('disabled', 'disabled').parent()
       	  				.append('<input type="hidden" name="coupon" value="'+code+'" />');
       	  				var unitName;
       	  				if(obj.type == 1) unitName = '%';
       	  				else unitName = ' Ft'; 
       	  				alert("Sikeres kuponbeváltás. A szolgáltatás díját csökkentettük a kupon értékével. (-"+obj.value+unitName+")");
       	  			}
       	  		}
       	  		catch(Exception)
       	  		{
       	  			alert('Sikertelen kuponbeváltás.');
       	  		}
       	  	});
       	  });
        });
        </script>                      </td>
        </tr>
      </table>
    </td>
  </tr>
