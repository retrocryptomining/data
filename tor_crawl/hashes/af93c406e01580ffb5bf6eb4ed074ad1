<script type="text/javascript">
    if(location.pathname != '/lending') {
        var is_promotion_viewed = $.cookie("is_promotion_viewed");

        //console.log('promotion: ', is_promotion_viewed);
        if(!is_promotion_viewed || is_promotion_viewed === false) {
            $.ajax({
                url: '' + '/promotion/check-status',
                type: 'get',
                success: function (response) {
                    if (response) {
                        swal({
                            title: 'Sweet!',
                            text: 'Etherbanking International Meetup!',
                            imageUrl: '/images/intro/promotion/promotion-1/promotion.gif',
                            imageWidth: 460,
                            imageAlt: 'Etherbanking International Meetup',
                            confirmButtonText: "Let's View",
                        }).then(function () {
                            $.cookie("is_promotion_viewed", true, {path: "/home"});
                            $.cookie("is_promotion_viewed", true, {path: "/"});
                            location.href = '' + '/promotion';
                        }, function () {
                            //console.log('no view');
                        });
                    }
                    else {
                        //console.log('no promotion to show');
                    }
                }
            });
        }


    }
</script>

