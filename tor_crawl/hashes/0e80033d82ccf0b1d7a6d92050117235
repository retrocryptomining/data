	function bookmark(address,sitename) {
	  if (window.sidebar) {
		window.sidebar.addPanel(sitename, address,"");
	  } else if( document.all ) {
		window.external.AddFavorite(address, sitename);
	  } else if( window.opera && window.print ) {
		return true;
	  }
	}

