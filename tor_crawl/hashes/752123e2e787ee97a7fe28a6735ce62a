<script type="text/javascript">
$(document).ready(function(){
    $(".nav_2 li:first a").css("border-left","0px");
    $(".nav_2 li:last a").css("border-right","0px");
	$(".nav_1 li").mouseover(function(){
		$(this).find(">a").addClass("nav_dq");
		$(this).find(".nav-all-list").css("display","block");
	})
	$(".nav_1 li").mouseleave(function(){
		$(this).find(">a").removeClass("nav_dq");
		$(this).find(".nav-all-list").css("display","none");
	})
	
var C = $("#addSiteFavorite");
var u = function () {
	var V = "安吉舆情";
	var U = "http://www.saitseo.com";
 	try {
        window.external.addFavorite(U, V);
    }catch (e) {
        try {
            window.sidebar.addPanel(V, U, "");
        }catch (e) {
            alert("抱歉，您所使用的浏览器无法完成此操作。\n\n加入收藏失败，请使用Ctrl+D进行添加");
        }
    }
};

C.bind("click", function () {
	u();
});
});
</script>
 
