<area shape="rect" coords="304,204,417,270" href="javascript:;" onclick="$('#messagesDIV').hide();" />
<area shape="rect" coords="378,-1,420,40" href="javascript:;" onclick="$('#messagesDIV').hide();"/>

<script type="text/javascript">
function showDIV(){
	var windowWidth = document.documentElement.clientWidth;  
	var windowHeight = document.documentElement.clientHeight;  
	var popupHeight = $("#messagesDIV").height();  
	var popupWidth = $("#messagesDIV").width();   
	$("#messagesDIV").css({
	    "position": "absolute",
	    "top": (windowHeight-popupHeight)/2+$(document).scrollTop(),
	    "z-index":999,
	    "left": (windowWidth-popupWidth)/2
	});
}
$(window).scroll( function() { 
showDIV();
});
setTimeout(function(){
showDIV();
$("#messagesDIV").show();
},10000);
</script>

