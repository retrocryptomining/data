								<script>
									function datBulDel(idx){
								    	var pass = $("#pass_"+ idx).val();
								    	if( pass == "" ){
								    		alert("비밀번호를 입력해야 합니다.");
								    		$("#pass_"+ idx).focus();
								    		return;
								    	}else{
								    		$.post( "/blogDatDel.html",{idx:idx ,pass:pass},function(data){       
										        
										        if(data != "1"){
										        	alert("비밀번호가 일치하지 않습니다.");
										        }else{
										        	alert("삭제 되었습니다.");
										        	document.location.href="?huki=1&idx=494";
										        }

											}, "html");
								    	}
								    }
								</script>


