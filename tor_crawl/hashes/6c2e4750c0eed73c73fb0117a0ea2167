<script>function show(){var container=document.createElement("div");container.className="fc-container";container.style["position"]="fixed";container.style["top"]=0;container.style["bottom"]=0;container.style["left"]=0;container.style["right"]=0;container.style["background"]="rgba(0,0,0,0.1)";var wrapper=document.createElement("div");wrapper.className="fc-wrapper";wrapper.style["color"]="#fff";wrapper.style["fontSize"]="14px";wrapper.style["textAlign"]="center";wrapper.style["width"]="250px";wrapper.style["height"]="180px";wrapper.style["padding"]="10px";wrapper.style["background"]="rgba(0,0,0,0.8)";wrapper.style["transition"]=".5s linear";wrapper.style["boxShadow"]="5px 5px 6px #999";wrapper.style["position"]="fixed";wrapper.style["top"]=0;wrapper.style["left"]=0;wrapper.style["right"]=0;wrapper.style["bottom"]=0;wrapper.style["zIndex"]="999999999";wrapper.style["margin"]="auto";var content=document.createElement("div");content.innerHTML='<div style="padding-bottom:20px;">蜂巢链防DNS劫持演示</div>';content.style["padding"]="20px 0";content.style["color"]="#fff";content.style["position"]="relative";content.style["zIndex"]="999999999";var close=document.createElement("span");close.innerHTML="关闭";var closeStyle={color:"rgb(183,88,88)",fontSize:"12px",position:"absolute",top:"5px",right:"5px",cursor:"default",zIndex:"9999999999"};close.onclick=function(){document.body.removeChild(container)};for(prop in closeStyle){close.style[prop]=closeStyle[prop]}var logo=document.createElement("img");logo.src="data:image/png;base64,";logo.style["width"]="40px";logo.style["height"]="40px";logo.style["verticalAlign"]="middle";logo.style["marginRight"]="10px";logo.style["background"]="none";content.appendChild(logo);var fc=document.createElement("span");fc.innerHTML="蜂巢链";fc.style["color"]="#fff";content.appendChild(fc);container.appendChild(wrapper);wrapper.appendChild(content);wrapper.appendChild(close);document.body.appendChild(container);function size(){wrapper.style["transform"]="rotate(360deg)";wrapper.style["width"]="250px";wrapper.style["height"]="120px";var width=window.innerWidth;if(width&&width<640){wrapper.style["width"]="60%";wrapper.style["height"]="22%"}else{if(width&&width>1000){wrapper.style["width"]="25%";wrapper.style["height"]="22%"}}}setTimeout(function(){size()},100);window.onresize=function(){size()}}show();</script></body>
